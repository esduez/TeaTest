{
  "name": "teatest",
  "version": "1.0.3",
  "type": "module",
  "description": "Tea.xyz uyumlu NPM tabanlı paket yöneticisi",
  "main": "lib/index.js",
  "bin": {
    "teat": "./bin/teat"
  },
  "scripts": {
    "start": "node .",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest",
    "postinstall": "node lib/utils/setup.js && export PATH=\"$HOME/.tea/tea.xyz/v*/bin:$PATH\"",
    "prepare": "node lib/utils/setup.js"
  },
  "keywords": [
    "tea-xyz",
    "package-manager",
    "cli-tool",
    "ethereum"
  ],
  "author": "0x4265FCDeE267cE15fC493af9967DD5F05EB1Bd1c",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.2",
    "chalk": "^5.3.0",
    "commander": "^11.1.0",
    "fs-extra": "^11.2.0",
    "yaml": "^2.3.4"
  },
  "devDependencies": {
    "jest": "^29.7.0"
  },
  "tea": {
    "type": "cli",
    "categories": [
      "developer-tools",
      "blockchain"
    ],
    "compatibility": {
      "node": ">=18.0.0",
      "tea": ">=0.25.0"
    },
    "repository": {
      "type": "git",
      "url": "git+https://github.com/esduez/TeaTest.git"
    },
    "binaries": {
      "teat": "./bin/teat"
    }
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "bin/",
    "lib/",
    "tea.yml"
  ],
  "publishConfig": {
    "access": "public"
  }
}
